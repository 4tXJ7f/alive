Name: MulNegOne:MulDivRem:555
%r = fmul %X, -1.0
  =>
%r = fsub -0.0, %X


; Ignore symmetric case
Name: NegMulNeg:MulDivRem:654-1
%a = fsub -0.0, %x
%b = fsub -0.0, %y
%r = fmul %a, %b
  =>
%r = fmul %x, %y


; Ignore symmetric case
Name: NegMulNeg:MulDivRem:654-2
Pre: AnyZero(C0) && AnyZero(C1)
%a = fsub C0, %x
%b = fsub C1, %y
%r = fmul nsz %a, %b
  =>
%r = fmul nsz %x, %y


; Ignore symmetric case
Name: NegMulNeg:MulDivRem:660-1
Pre: hasOneUse(%a)
%a = fsub -0.0, %x
%r = fmul %a, %y
  =>
%b = fmul %x, %y
%r = fsub -0.0, %b


; Ignore symmetric case
Name: NegMulNeg:MulDivRem:660-2
Pre: hasOneUse(%a) && AnyZero(C0)
%a = fsub C0, %x
%r = fmul nsz %a, %y
  =>
%b = fmul nsz %x, %y
%r = fsub -0.0, %b
